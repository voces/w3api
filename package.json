{
  "name": "w3api",
  "version": "1.2.2",
  "description": "TypeScript API for Warcraft III natives",
  "author": "verit",
  "license": "MIT",
  "scripts": {
    "build": "tsc; ts-node bin/build.ts",
    "dev": "npm-watch",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.26",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-mdcs": "^4.2.3",
    "eslint-config-verit": "^1.0.3",
    "fast-glob": "^3.1.1",
    "jest": "^24.9.0",
    "npm-watch": "^0.6.0",
    "recast": "^0.18.5",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.6.2",
    "ts-transformer-imports": "^0.4.3",
    "typescript": "^3.7.5"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "legacyWatch": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voces/w3api.git"
  },
  "bugs": {
    "url": "https://github.com/voces/w3api/issues"
  },
  "homepage": "https://github.com/voces/w3api#readme",
  "files": [
    "dist"
  ],
  "main": "dist/index.ts",
  "dependencies": {
    "w3xdata": "^2.1.2"
  }
}
