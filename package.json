{
  "name": "w3api",
  "version": "1.4.0",
  "description": "TypeScript API for Warcraft III natives",
  "author": "verit",
  "license": "MIT",
  "scripts": {
    "build": "tsc; ts-node bin/build.ts",
    "dev": "npm-watch",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-mdcs": "^4.2.3",
    "eslint-config-verit": "^1.0.4",
    "fast-glob": "^3.2.2",
    "jest": "^25.2.4",
    "npm-watch": "^0.6.0",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "legacyWatch": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voces/w3api.git"
  },
  "bugs": {
    "url": "https://github.com/voces/w3api/issues"
  },
  "homepage": "https://github.com/voces/w3api#readme",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "w3xdata": "^2.1.3"
  }
}
